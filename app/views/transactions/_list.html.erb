<div id = "list_container">
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Vendor</th>
      <th>Who</th>
      <th>What</th>
      <th>Where</th>
      <th>Why</th>
      <th>Amount</th>
      <th>Program</th>
      <th>Account</th>
      <th>Purchased by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="list_of_items">
    <% @transactions.each do |transaction| %>
      <%= render 'list_item', transaction: transaction %>
    <% end %>
  </tbody>
</table>

</div>
<div class = "paginate"><%= will_paginate @transactions %></div>
